<!DOCTYPE html>
<html>
<head>
    <title><%= project.name %></title>
</head>
<body>
    <h1><%= project.name %></h1>
    <p><%= project.description %></p>
    <h2>Tareas</h2>
    <ul>
        <% project.tasks.forEach(function(task) { %>
            <li>
                <%= task.name %> - Estado: <%= task.status %>
            </li>
        <% }); %>
    </ul>
</body>
</html>